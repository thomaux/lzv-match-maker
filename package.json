{
  "name": "lzv-match-maker",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prewatch": "rm -r -f lib",
    "watch": "concurrently \"tsc -p src/server/tsconfig.server.json -w\" \"nodemon lib\"",
    "prestart": "tsc -p src/server/tsconfig.server.json",
    "start": "node -r dotenv/config lib",
    "populate-reference-collections": "npx ts-node -r dotenv/config scripts/populate-region-reference-collections.ts"
  },
  "private": true,
  "dependencies": {
    "ajv": "^6.10.0",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "lodash": "^4.17.15",
    "mongoose": "^5.4.14",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/lodash": "^4.14.121",
    "@types/mongoose": "^5.3.18",
    "@types/node": "^11.9.4",
    "@types/passport": "^1.0.0",
    "@types/passport-facebook": "^2.1.8",
    "@types/request-promise-native": "^1.0.15",
    "typescript": "^3.3.3333"
  }
}
