{
  "name": "lzv-match-maker",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prewatch": "rm -r -f lib",
    "watch": "concurrently \"tsc -p src/server/tsconfig.server.json -w\" \"nodemon lib\"",
    "prestart": "rm -r -f lib && tsc -p src/server/tsconfig.server.json && cp -R src/server/public lib/public",
    "start": "node -r dotenv/config lib/main",
    "populate-reference-collections": "npx ts-node -r dotenv/config scripts/populate-region-reference-collections.ts"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^6.6.7",
    "@nestjs/core": "^6.6.7",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.6.7",
    "@nestjs/serve-static": "^1.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "connect-mongo": "^2.0.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@types/connect-mongo": "0.0.43",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/lodash": "^4.14.121",
    "@types/mongoose": "^5.5.17",
    "@types/node": "^11.9.4",
    "@types/passport": "^1.0.0",
    "@types/passport-facebook": "^2.1.8",
    "@types/request-promise-native": "^1.0.15",
    "tslint": "^5.20.0",
    "typescript": "^3.3.3333"
  }
}
